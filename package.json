{
  "name": "ng2-workshop",
  "version": "1.0.0",
  "description": "Angular 2 workshop",
  "main": "index.js",
  "scripts": {
    "start": "node tasks/server",
    "test": "karma start karma.conf.js",
    "setup": "npm install && jspm install",
    "build": "gulp build-assets && node tasks/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thalen/ng2-base"
  },
  "author": "Olle Thal√©n",
  "license": "MIT",
  "devDependencies": {
