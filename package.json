    "angular2-in-memory-web-api": "0.0.21",
    "chokidar-socket-emitter": "^0.5.4",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "json-typescript-mapper": "^1.0.5",
    "jspm": "^0.17.0-beta.22",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-uiuxengineering-jspm": "^3.2.0",
    "moment": "^2.15.1",
    "ng2-bootstrap": "^1.1.14",
    "ng2-cookies": "^1.0.3",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.7",
    "rxjs": "^5.0.0-beta.12",
    "sass": "^0.5.0",
    "sass-jspm-importer": "0.0.6",
    "typescript-collections": "^1.2.3",
    "zone.js": "^0.6.25"
  },
  "jspm": {
    "format": "register",
    "main": "plugin",
    "registry": "github",
    "name": "ng2-base",
    "directories": {
      "lib": "lib",
      "dist": "lib"
    },
    "dependencies": {
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.0.1",
      "@angular/router": "npm:@angular/router@^3.0.1",
      "angular2-in-memory-web-api": "npm:angular2-in-memory-web-api@^0.0.21",
      "bootstrap-sass": "npm:bootstrap-sass@^3.3.7",
      "es6-shim": "es-shims/es6-shim@^0.35.1",
      "frankwallis/plugin-typescript": "5.1.2",
      "json-typescript-mapper": "npm:json-typescript-mapper@^1.0.5",
      "moment": "npm:moment@2.15.1",
      "ng2-bootstrap": "npm:ng2-bootstrap@^1.1.14",
      "ng2-cookies": "npm:ng2-cookies@^1.0.3",
      "scss": "mobilexag/plugin-sass@^0.4.6",
      "typescript-collections": "npm:typescript-collections@^1.2.3"
    },
    "devDependencies": {
      "font-awesome": "npm:font-awesome@^4.6.3",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12",
      "systemjs-hot-reloader": "capaj/systemjs-hot-reloader@0.5.3",
      "text": "systemjs/plugin-text@^0.0.9",
      "ts": "frankwallis/plugin-typescript@^5.1.0"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@2.0.1",
      "@angular/compiler": "npm:@angular/compiler@2.0.1",
      "@angular/core": "npm:@angular/core@2.0.1",
      "@angular/forms": "npm:@angular/forms@^2.0.0",
      "@angular/http": "npm:@angular/http@2.0.1",
      "@angular/platform-browser": "npm:@angular/platform-browser@^2.0.1",
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "child_process": "npm:jspm-nodelibs-child_process@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "module": "npm:jspm-nodelibs-module@^0.2.0",
      "net": "npm:jspm-nodelibs-net@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "jspm/nodelibs-process@^0.2.0-alpha",
      "reflect-metadata": "npm:reflect-metadata@^0.1.0",
      "rxjs": "npm:rxjs@5.0.0-beta.12",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "timers": "npm:jspm-nodelibs-timers@^0.2.0",
      "tty": "npm:jspm-nodelibs-tty@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zone.js": "npm:zone.js@^0.6.21"
    },
    "overrides": {
      "npm:@angular/common@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/common": ".",
          "./testing": "./bundles/common-testing.umd.js"
        }
      },
      "npm:@angular/compiler@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/compiler": ".",
          "./testing": "./bundles/compiler-testing.umd.js"
        }
      },
      "npm:@angular/core@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/core": ".",
          "./testing": "./bundles/core-testing.umd.js"
        }
      },
      "npm:@angular/http@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/http": ".",
          "./testing": "./bundles/http-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser-dynamic@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser-dynamic": ".",
          "./testing": "./bundles/platform-browser-dynamic-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser@2.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser": ".",
          "./testing": "./bundles/platform-browser-testing.umd.js"
        }
      },
      "npm:@angular/router@3.0.1": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/router": ".",
          "./testing": "./bundles/router-testing.umd.js"
        }
      },
      "npm:babel-core@5.8.38": {
        "main": "browser.js",
        "dependencies": {},
        "jspmNodeConversion": false,
        "map": {
          "regenerator/runtime": "babel-runtime/regenerator/runtime"
        },
        "meta": {
          "browser.js": {
            "exports": "babel",
            "format": "global"
          }
        }
      },
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:font-awesome@4.6.3": {
        "main": "css/font-awesome.css!",
        "jspmNodeConversion": false,
        "registry": "jspm",
        "dependencies": {
          "css": "0.1"
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.16.3": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.15.1": {
        "jspmNodeConversion": false
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.8": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      },
      "npm:sass.js@0.9.13": {
        "map": {
          "ws": "@empty"
        },
        "meta": {
          "dist/sass.worker.js": {
            "format": "global"
          }
        }
      },
      "npm:typescript@2.0.3": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
